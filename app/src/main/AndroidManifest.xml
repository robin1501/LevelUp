<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.krro.levelup" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.krro.levelup.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="EinstellungenActivity"></activity>
        <activity android:name="KalenderActivity"></activity>
        <activity android:name="ProfilActivity"></activity>
        <activity android:name="UebersichtActivity"></activity>
        <activity android:name="UebungenActivity"></activity>
        <activity android:name="UebungenDetail"></activity>
        <activity android:name="WorkoutActivity"></activity>
    </application>

</manifest>
